<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns:fx="http://javafx.com/fxml" fx:controller="com.example.apibrawlstars.Controller.RegisterModalController" prefHeight="250.0" prefWidth="400.0">
    <children>
        <TextField fx:id="txtNombre" layoutX="20.0" layoutY="20.0" promptText="Nombre" />
        <PasswordField fx:id="txtContrasena" layoutX="20.0" layoutY="70.0" promptText="Contraseña" />
        <PasswordField fx:id="txtContrasenaConfirm" layoutX="20.0" layoutY="120.0" promptText="Confirmar Contraseña" />
        <Button fx:id="btnRegistrarse" layoutX="150.0" layoutY="180.0" text="Registrarse" onAction="#onBtnClickRegistrarse" />
    </children>
</AnchorPane>