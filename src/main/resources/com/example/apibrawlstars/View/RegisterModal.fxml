    <children>
        <TextField fx:id="txtNombre" layoutX="113.0" layoutY="62.0" promptText="Nombre" />
        <PasswordField fx:id="txtContrasena" layoutX="113.0" layoutY="113.0" promptText="Contraseña" />
        <PasswordField fx:id="txtContrasenaConfirm" layoutX="113.0" layoutY="148.0" promptText="Confirmar Contraseña" />
        <Button fx:id="btnRegistrarse" layoutX="150.0" layoutY="192.0" onAction="#onBtnClickRegistrarse" text="Registrarse" />
    </children>
</AnchorPane>
